{
    "version": 2,
    "builds": [
	{ "src": "package.json", "use": "@now/static-build"},
	{ "src": "./index.js", "use": "@now/node-server" },
   { "src": "./app.js", "use": "@now/node-server" },
   { "src": "./routes/*.js", "use": "@now/node-server" }
    ],
    "routes": [
	{ "src": "/", "dest": "./routes/index.js" },
    { "src": "/blogs", "dest": "/" },
    { "src": "/blogs/1", "dest": "/" },
   { "src": "/contact", "dest": "/" },
   { "src": "/about", "dest": "/" },
   { "src": "/services/tracking", "dest": "/" },
   { "src": "/services/traking/shipment", "dest": "/" },
   { "src": "/services/checkout", "dest": "/" },
   { "src": "/services/confirm", "dest": "/" },
   { "src": "/shop", "dest": "/" },
   { "src": "/shop/cart", "dest": "/" },
   { "src": "/shop/1", "dest": "/" },
   { "src": "/users", "dest": "/" },
   { "src": "/users/login", "dest": "/" },
   { "src": "/users/login/account", "dest": "/" },
   { "src": "/users/login/account/confirm", "dest": "/" },
   { "src": "/users/forgot", "dest": "/" },
   { "src": "/users/forgot/confirm", "dest": "/" },
   { "src": "/users/register", "dest": "/" },
   { "src": "/users/register/confirm", "dest": "/" }
    ]
}
